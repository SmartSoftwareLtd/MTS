            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            type: 'POST',
            url: '/VehicleStatus/GetSoldVehicles/',
            data: JSON.stringify({ 'fromDate': fromDate, 'ToDate': toDate }),
            success: function (data) {
            
                debugger
